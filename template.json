{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "virtualMachines_linuxds4_adminPassword": {
            "defaultValue": null,
            "type": "SecureString"
        },
        "virtualMachines_linuxds4_name": {
            "defaultValue": "linuxds4",
            "type": "String"
        },
        "networkInterfaces_linuxds4571_name": {
            "defaultValue": "linuxds4571",
            "type": "String"
        },
        "networkSecurityGroups_linuxds4_nsg_name": {
            "defaultValue": "linuxds4-nsg",
            "type": "String"
        },
        "publicIPAddresses_linuxds4_ip_name": {
            "defaultValue": "linuxds4-ip",
            "type": "String"
        },
        "virtualNetworks_linuxds4_vnet_name": {
            "defaultValue": "linuxds4-vnet",
            "type": "String"
        },
        "storageAccounts_linuxds4694_name": {
            "defaultValue": "linuxds4694",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "comments": "Generalized from resource: '/subscriptions/9fe2c50a-49af-401a-89d9-9360283d0bc6/resourceGroups/linuxds4/providers/Microsoft.Compute/virtualMachines/linuxds4'.",
            "type": "Microsoft.Compute/virtualMachines",
            "name": "[parameters('virtualMachines_linuxds4_name')]",
            "apiVersion": "2015-06-15",
            "location": "eastasia",
            "plan": {
                "name": "linuxdsvm",
                "product": "linux-data-science-vm",
                "publisher": "microsoft-ads"
            },
            "properties": {
                "hardwareProfile": {
                    "vmSize": "Standard_A3"
                },
                "storageProfile": {
                    "imageReference": {
                        "publisher": "microsoft-ads",
                        "offer": "linux-data-science-vm",
                        "sku": "linuxdsvm",
                        "version": "latest"
                    },
                    "osDisk": {
                        "name": "[parameters('virtualMachines_linuxds4_name')]",
                        "createOption": "FromImage",
                        "vhd": {
                            "uri": "[concat('https', '://', parameters('storageAccounts_linuxds4694_name'), '.blob.core.windows.net', concat('/vhds/', parameters('virtualMachines_linuxds4_name'),'201672416240.vhd'))]"
                        },
                        "caching": "ReadWrite"
                    },
                    "dataDisks": []
                },
                "osProfile": {
                    "computerName": "[parameters('virtualMachines_linuxds4_name')]",
                    "adminUsername": "zhle",
                    "linuxConfiguration": {
                        "disablePasswordAuthentication": true,
                        "ssh": {
                            "publicKeys": [
                                {
                                    "path": "/home/zhle/.ssh/authorized_keys",
                                    "keyData": "ssh-rsa "
                                }
                            ]
                        }
                    },
                    "secrets": [],
                    "adminPassword": "[parameters('virtualMachines_linuxds4_adminPassword')]"
                },
                "networkProfile": {
                    "networkInterfaces": [
                        {
                            "id": "[resourceId('Microsoft.Network/networkInterfaces', parameters('networkInterfaces_linuxds4571_name'))]"
                        }
                    ]
                }
            },
            "resources": [],
            "dependsOn": [
                "[resourceId('Microsoft.Storage/storageAccounts', parameters('storageAccounts_linuxds4694_name'))]",
                "[resourceId('Microsoft.Network/networkInterfaces', parameters('networkInterfaces_linuxds4571_name'))]"
            ]
        },
        {
            "comments": "Generalized from resource: '/subscriptions/9fe2c50a-49af-401a-89d9-9360283d0bc6/resourceGroups/linuxds4/providers/Microsoft.Network/networkInterfaces/linuxds4571'.",
            "type": "Microsoft.Network/networkInterfaces",
            "name": "[parameters('networkInterfaces_linuxds4571_name')]",
            "apiVersion": "2016-03-30",
            "location": "eastasia",
            "properties": {
                "ipConfigurations": [
                    {
                        "name": "ipconfig1",
                        "properties": {
                            "privateIPAddress": "10.3.0.4",
                            "privateIPAllocationMethod": "Dynamic",
                            "publicIPAddress": {
                                "id": "[resourceId('Microsoft.Network/publicIPAddresses', parameters('publicIPAddresses_linuxds4_ip_name'))]"
                            },
                            "subnet": {
                                "id": "[concat(resourceId('Microsoft.Network/virtualNetworks', parameters('virtualNetworks_linuxds4_vnet_name')), '/subnets/default')]"
                            }
                        }
                    }
                ],
                "dnsSettings": {
                    "dnsServers": []
                },
                "enableIPForwarding": false,
                "networkSecurityGroup": {
                    "id": "[resourceId('Microsoft.Network/networkSecurityGroups', parameters('networkSecurityGroups_linuxds4_nsg_name'))]"
                }
            },
            "resources": [],
            "dependsOn": [
                "[resourceId('Microsoft.Network/publicIPAddresses', parameters('publicIPAddresses_linuxds4_ip_name'))]",
                "[resourceId('Microsoft.Network/virtualNetworks', parameters('virtualNetworks_linuxds4_vnet_name'))]",
                "[resourceId('Microsoft.Network/networkSecurityGroups', parameters('networkSecurityGroups_linuxds4_nsg_name'))]"
            ]
        },
        {
            "comments": "Generalized from resource: '/subscriptions/9fe2c50a-49af-401a-89d9-9360283d0bc6/resourceGroups/linuxds4/providers/Microsoft.Network/networkSecurityGroups/linuxds4-nsg'.",
            "type": "Microsoft.Network/networkSecurityGroups",
            "name": "[parameters('networkSecurityGroups_linuxds4_nsg_name')]",
            "apiVersion": "2016-03-30",
            "location": "eastasia",
            "properties": {
                "securityRules": [
                    {
                        "name": "Jupyter",
                        "properties": {
                            "protocol": "TCP",
                            "sourcePortRange": "*",
                            "destinationPortRange": "9999",
                            "sourceAddressPrefix": "*",
                            "destinationAddressPrefix": "*",
                            "access": "Allow",
                            "priority": 1010,
                            "direction": "Inbound"
                        }
                    },
                    {
                        "name": "JupyterHub",
                        "properties": {
                            "protocol": "TCP",
                            "sourcePortRange": "*",
                            "destinationPortRange": "8000",
                            "sourceAddressPrefix": "*",
                            "destinationAddressPrefix": "*",
                            "access": "Allow",
                            "priority": 1020,
                            "direction": "Inbound"
                        }
                    },
                    {
                        "name": "default-allow-ssh",
                        "properties": {
                            "protocol": "TCP",
                            "sourcePortRange": "*",
                            "destinationPortRange": "22",
                            "sourceAddressPrefix": "*",
                            "destinationAddressPrefix": "*",
                            "access": "Allow",
                            "priority": 1030,
                            "direction": "Inbound"
                        }
                    }
                ]
            },
            "resources": [],
            "dependsOn": []
        },
        {
            "comments": "Generalized from resource: '/subscriptions/9fe2c50a-49af-401a-89d9-9360283d0bc6/resourceGroups/linuxds4/providers/Microsoft.Network/publicIPAddresses/linuxds4-ip'.",
            "type": "Microsoft.Network/publicIPAddresses",
            "name": "[parameters('publicIPAddresses_linuxds4_ip_name')]",
            "apiVersion": "2016-03-30",
            "location": "eastasia",
            "properties": {
                "publicIPAllocationMethod": "Dynamic",
                "idleTimeoutInMinutes": 4,
                "dnsSettings": {
                    "domainNameLabel": "linuxds4"
                }
            },
            "resources": [],
            "dependsOn": []
        },
        {
            "comments": "Generalized from resource: '/subscriptions/9fe2c50a-49af-401a-89d9-9360283d0bc6/resourceGroups/linuxds4/providers/Microsoft.Network/virtualNetworks/linuxds4-vnet'.",
            "type": "Microsoft.Network/virtualNetworks",
            "name": "[parameters('virtualNetworks_linuxds4_vnet_name')]",
            "apiVersion": "2016-03-30",
            "location": "eastasia",
            "properties": {
                "addressSpace": {
                    "addressPrefixes": [
                        "10.3.0.0/16"
                    ]
                },
                "subnets": [
                    {
                        "name": "default",
                        "properties": {
                            "addressPrefix": "10.3.0.0/24"
                        }
                    }
                ]
            },
            "resources": [],
            "dependsOn": []
        },
        {
            "comments": "Generalized from resource: '/subscriptions/9fe2c50a-49af-401a-89d9-9360283d0bc6/resourceGroups/linuxds4/providers/Microsoft.Storage/storageAccounts/linuxds4694'.",
            "type": "Microsoft.Storage/storageAccounts",
            "sku": {
                "name": "Standard_LRS",
                "tier": "Standard"
            },
            "kind": "Storage",
            "name": "[parameters('storageAccounts_linuxds4694_name')]",
            "apiVersion": "2016-01-01",
            "location": "eastasia",
            "tags": {},
            "properties": {},
            "resources": [],
            "dependsOn": []
        }
    ]
}
